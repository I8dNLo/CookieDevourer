{
  "manifest_version": 3,
  "name": "CookieD",
  "version": "1.0",
  "description": "View cookies for the current website.",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "cookies",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png"
    }
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
